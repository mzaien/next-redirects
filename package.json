{
  "name": "next-redirects",
  "version": "3.2.2",
  "private": false,
  "description": "A small Redirect utility component for Next.js",
  "source": "src/index.tsx",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsdx build tsdx build --entry src/index.tsx --name index ",
    "prebuild": "rm -rf dist && tsc --noEmit -p tsconfig.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mzaien/next-redirects.git"
  },
  "keywords": [
    "next",
    "next.js",
    "react",
    "redirect",
    "link",
    "router"
  ],
  "author": "Abdullah Mzaien <awmzaien@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mzaien/next-redirects/issues"
  },
  "peerDependencies": {
    "next": ">= 10.2.3",
    "react": ">= 16.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.15",
    "eslint": "^8.20.0",
    "eslint-config-next": "12.3.0",
    "next": "^12.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "^4.7.4"
  }
}
